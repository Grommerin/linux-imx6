#include "imx6q-marsboard.dtsi"

/ {
  	model = "Marsboard i.MX6 Dual Board";
	compatible = "embest,imx6q-marsboard", "fsl,imx6q";
	//aliases {
	//	mxcfb0 = &mxcfb1;
	//	mxcfb1 = &mxcfb2;
